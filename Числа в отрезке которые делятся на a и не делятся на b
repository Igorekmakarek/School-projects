//Рассматривается множество целых чисел, принадлежащих отрезку [1606; 9680], 
//которые делятся на 11 и не делятся на 7, 13, 17, 19. Найдите количество таких чисел и максимальное из них.
// В ответе запишите два числа через пробел: сначала количество, затем максимальное число

#include <iostream>

using namespace std;

int main() {
    int k = 0;
    int numarr[736];
    int max;
    for (int i = 1606; i <= 9680; i++) {
        if ((i % 11 == 0) && (i % 7 != 0 && i % 13 != 0 && i % 17 != 0 && i % 19 != 0)) {
            k++;
            numarr[k] = i;
            max = i;
        }
    }

    cout << k << " " << max << endl;
    
getchar();
return 0;
}
