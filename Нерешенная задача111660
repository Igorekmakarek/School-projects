#include <iostream>
#include "math.h"

using namespace std;

int main()
{
    int m;
    int N;
    cin >> N;
    for (m = 0; N > 0; m++) {
        N = N / 10;
    }
    int num[m];
    for (int m; m > 0; m--) {
        int i = 0;
        int d;
        d = pow(10, m);
        cout << d;
        num[i] = N % d;
        i++;
    }
    // for (int i = 0; i < m; i++)
    // cout << num[i] << endl;
    
    
    

    return 0;
}
